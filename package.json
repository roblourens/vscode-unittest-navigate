{
    "name": "unittest-navigate",
    "displayName": "Unit Test Navigate",
    "description": "Adds your unit test definition calls to the symbols list for easier navigation of javascript and typescript unit test scripts.",
    "version": "0.0.2",
    "publisher": "roblourens",
    "engines": {
        "vscode": "^1.5.0"
    },
    "icon": "images/mocha.svg",
    "categories": [
        "Languages",
        "Other"
    ],
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:javascript"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "homepage": "https://github.com/roblourens/vscode-unittest-navigate",
    "bugs": {
        "url": "https://github.com/roblourens/vscode-unittest-navigate/issues"
    },
    "repository": {
        "url": "https://github.com/roblourens/vscode-unittest-navigate/issues"
    }
}